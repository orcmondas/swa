{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "routes": [
    {
      "route": "/sparql/*",
      "rewrite": "/api/sparql/{*}",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  }
}